{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Strony"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "tytul": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "unique": false
    },
    "banner": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "tytul",
      "required": true
    },
    "pozycja": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "unique": true
    },
    "tresc": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "template": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": "default",
      "enum": [
        "contact",
        "blog",
        "about",
        "default",
        "home"
      ]
    }
  }
}
